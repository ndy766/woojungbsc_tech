    <script>
        function sendReport(report_state){
            var frm = document.getElementById('report_form');
            var report_state = document.getElementById('report_state').value;

            if(report_state=='send'){
                var flag = confirm('이미 전송된 A/S리포트입니다. 재 전송하시겠습니까?');
                if(!flag){
                    return;
                }else{
                    frm.submit();
                }
            }else{
                var flag2 = confirm('정말 A/S리포트를 메일로 전송하시겠습니까?');
                if(!flag2){
                    return;
                }else{
                    frm.submit();
                }
            }

        }
