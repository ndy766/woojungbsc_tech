    <script>
        function confirmDeleteSchedule(){
            var no = document.getElementById('no').value;
            if(confirm("정말 삭제하시겠습니까?")==true){
                location.href="/calendar/delete?no="+no;
            }else{
                return;
            }
        }
    </script>

    <script type="text/javascript">
        function checkForm(work_type){
            var type = work_type.value;
            var form_list = document.getElementsByClassName('calendar_form');
            for(var i=0;i<form_list.length;i++){
                form_list[i].setAttribute('style','display:none');
            }

            if(type=='무상유지보수' || type=='유상유지보수'){
                document.getElementById('form_as').setAttribute('style', 'display:show');
            }else if(type=='RBDS'){
                document.getElementById('form_RBDS').setAttribute('style', 'display:show');
            }else if(type=='내근업무'){
                document.getElementById('form_inner').setAttribute('style', 'display:show');
            }else{
                document.getElementById('form_as').setAttribute('style', 'display:show');
            }

        }

    </script>

    <script>
        function submitSchedule(){
            var type = document.getElementById('work_type').value;
            var form_type = '';

            if(type=='무상유지보수' || type=='유상유지보수'){
                form_type='form_as';
            }else if(type=='RBDS'){
                form_type='form_RBDS';
            }else if(type=='내근업무'){
                form_type='form_inner';
            }else{
                form_type='form_as';
            }

            var form_list = document.getElementsByClassName('calendar_form');
            for(var i=0;i<form_list.length;i++){
                if(form_list[i].getAttribute('id')==form_type){
                    continue;
                }else {
                    form_list[i].innerHTML = '';
                }
            }
            var scheduleModifyForm = document.getElementById('scheduleModifyForm');
            console.log(scheduleModifyForm.innerHTML);

            scheduleModifyForm.submit();
        }
    </script>

    <script>
        function reportForm(no){
            var flag = confirm('A/S리포트를 작성하시겠습니까?(수정사항은 "일정 수정"버튼을 클릭해야 최종 반영됩니다.');
            if(!flag) return;

            var no = no;
            var work_type = document.getElementById('work_type').value;
            if(work_type != '유상유지보수' && work_type != '무상유지보수'){
                alert("A/S업무만 리포트 작성이 가능합니다.");
                return;
            }
            location.href = "/calendar/reportForm?no="+no;
        }

    </script>
