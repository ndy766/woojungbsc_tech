    <script>
        function modifyCode(no){
            var code_no = no;

            var code_name = prompt('수정할 항목의 이름을 입력하세요');
            var code_type = prompt('등록할 항목의 타입을 입력하세요(업무구분, 제조사, 미결사유)');

            if(code_name!=null && code_name!="" && code_name!=null && code_type!=null && code_type!="" && code_type!=null)
                $.ajax({
                    url: '/code/modify',
                    dataType: 'json',
                    type:'post',
                    data: {
                        code_no:code_no,
                        code_name:code_name,
                        code_type:code_type
                    },
                    success:function(data){
                        location.reload();
                    }

                });
        }

        function deleteCode(no){
            var code_no = no;

            var flag = confirm("정말로 삭제하시겠습니까?");
            if(!flag) return;

            $.ajax({
                url: '/code/delete?code_no='+code_no,
                dataType: 'json',
                type:'get',
                success:function(data){
                    location.reload();
                }

            })
        }

        function registerCode(){
            var code_name = prompt('등록할 항목의 이름을 입력하세요');
            var code_type = prompt('등록할 항목의 타입을 입력하세요(업무구분, 제조사, 미결사유)');
            if(code_name!=null && code_name!="" && code_name!=null && code_type!=null && code_type!="" && code_type!=null)
                $.ajax({
                    url: '/code/register',
                    dataType: 'json',
                    type:'post',
                    data: {
                        code_name:code_name,
                        code_type:code_type
                    },
                    success:function(data){
                        location.reload();
                    }

                });
        }
    </script>


